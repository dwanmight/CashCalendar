<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.might.dwan.cashcalendar"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:name="com.might.dwan.cashcalendar.utils.PayCounterApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.might.dwan.cashcalendar.ui.activities.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.might.dwan.cashcalendar.ui.activities.DetailPayActivity"
                  android:screenOrientation="portrait"/>
        <activity android:name="com.might.dwan.cashcalendar.ui.activities.UpdateProfileActivity"
                  android:parentActivityName=".ui.activities.MainActivity"
                  android:screenOrientation="portrait"/>
    </application>

</manifest>